<!DOCTYPE html>
<html>
  <head>
    <title>FlatironPictureGram</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="container">
    <div class="grid">
      <div class="header col_4 cf">

        <div class="logo">
        <a href=""><img src="data:image/png;base64," alt="Flatiron App Logo"/></a>
        </div>

        <nav>
        <ul class="navMenu">
          <li class="navMenu_item"><%= link_to 'Log in', '/login' %></li>
          <li class="navMenu_item_button"><%= link_to 'Sign up', '/signup'%></li>
        </ul>
        </nav><!-- end nav -->

      </div><!-- end header -->

    </div><!-- end grid -->

    <%= yield %>

    </div><!-- end contianer -->

  </body>
</html>
